{
     "name": "mark-villudo/laravel-user-management",
    "description": "Permission handling for Laravel 5.1 and up",
    "keywords": [
        "spatie",
        "laravel",
        "permission",
        "acl",
        "security"
    ],
    "homepage": "https://github.com/MarkVilludo/laravel-user-management",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Villudo",
            "email": "markanthony.villudo@gmail.com",
            "role": "Backend developer"
        }
    ],
    "require": {
        "php" : ">=5.6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.3.0",
        "phpunit/phpunit" : "^5.7.8|~6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "MarkVilludo\\Permission\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarkVilludo\\Permission\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarkVilludo\\Permission\\PermissionServiceProvider"
            ]
        }
    }
}

